{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "module": "es6",
    "target": "es6",
    "lib": ["esnext", "dom"],
    "downlevelIteration": true,
    "jsx": "react",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
